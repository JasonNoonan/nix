{
  "$schema": "https://opencode.ai/config.json",
  "theme": "catppuccin",
  "model": "anthropic/claude-sonnet-4-20250514",
  "autoupdate": true,
  "instructions": [],
  "layout": "stretch",
  "mcp": {
    "puppeteer": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "@smithery/cli@latest",
        "run",
        "@smithery-ai/puppeteer",
        "--key",
        "f3ffb009-4859-495a-bc7a-88040c454666"
      ],
      "enabled": true
    },
    "time": {
      "type": "local",
      "command": ["docker", "run", "-i", "--rm", "mcp/time"],
      "enabled": true
    },
    "magicuidesign": {
      "type": "local",
      "command": ["npx", "-y", "@magicuidesign/mcp@latest"],
      "enabled": true
    },
    "sequential-thinking": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "enabled": true
    },
    "git": {
      "type": "local",
      "command": [
        "docker",
        "run",
        "--rm",
        "-i",
        "--mount",
        "type=bind,src=/Users/jasonnoonan,dst=/Users/jasonnoonan",
        "mcp/git"
      ],
      "enabled": true
    }
  }
}
